<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.ConsumeMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.pojo.Consume">
            <result property="consumption" column="consumption" jdbcType="DECIMAL"/>
            <result property="id" column="id" jdbcType="INTEGER"/>
            <result property="tTime" column="t_time" jdbcType="TIMESTAMP"/>
            <result property="sign" column="sign" jdbcType="SMALLINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        consumption,id,t_time,
        sign
    </sql>
</mapper>
